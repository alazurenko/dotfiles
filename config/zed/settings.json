{
  "show_inline_completions": false,
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2",
    "provider": {
      "name": "openai",
      "default_model": "gpt-4"
    }
  },
  "autosave": "on_focus_change",
  "buffer_font_family": "Fira Code",
  "project_panel": {
    "dock": "right"
  },
  "theme": "Catppuccin Macchiato",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "format_on_save": "on",
  "vim_mode": true,
  "relative_line_numbers": true,
  "ui_font_size": 18,
  "buffer_font_size": 18
}
